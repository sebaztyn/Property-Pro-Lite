{
  "name": "Property-pro-lite",
  "version": "1.0.0",
  "description": "",
  "main": "Server/app.js",
  "scripts": {
    "try-test": "nyc mocha Server/test/*.js",
    "clean": "rm -rf build && mkdir build",
    "copy-swagger": "cp -R swagger.yaml build",
    "start": "node build/app.js",
    "start-dev": "nodemon --exec babel-node Server/app.js",
    "test": "nyc _mocha Server/test/*.test.js --reporter=spec --require @babel/register --exit",
    "build": "babel Server --out-dir build",
    "dev-start": "babel-watch Server/app.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "generate-lcov": "nyc report --reporter=text-lcov >lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "engines": {
    "node": "8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebaztyn/Property-Pro-Lite.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bcryptjs": "^2.4.3",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.0",
    "pg": "^7.9.0",
    "swagger-ui-express": "^4.0.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-watch": "^7.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^14.1.1"
  }
}